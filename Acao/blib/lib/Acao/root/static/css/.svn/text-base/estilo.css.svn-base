#topo
{
    font-family: Arial, Helvetica, sans-serif;
    margin: 0px auto 0px;
    padding-top: 0px;
    height: 100px;
    /*width: 760px;*/
    background: #D40000 url(../img/bg_topo2.png) top left repeat-x;
}

#topo img {
	margin:2px;
	float:left;
}

#topoTitulo
{
    padding-top: 20px;
    font-size: 100%;
    font-weight: bold;
    text-transform: uppercase;
    text-shadow: 1px 1px #CC0000;
    letter-spacing: 1px;
    color: #FFFFFF;
	float: right;
	margin-right: 30px;
}

#topoTitulo table
{
    border: 0px;
    padding: 0px;
    margin: 5px 0px 10px;
    font-size: 75%;
    font-weight: normal;
    text-transform: none;
}

#barraTopo
{
    margin: 0px auto 0px;
    height: 30px;
    /*width:760px;*/
    /*background: #E6B800 url(../img/bgMenuTopo.png) center top no-repeat;*/
    background: #E6B800;
    z-index: 10;
}

#barraTopo ul
{
    margin-top: 0px;
    padding-left:30px;
	padding-right: 10px;
    padding-top: 5px;
    margin-bottom: 0px;
    list-style:none;
}

#barraTopo ul li:hover
{
    overflow: visible;
}

#barraTopo ul li
{
    overflow: hidden;
    line-height: 1.5em;
    font-size:90%;
    display: block;
    float: left;
    position: relative;
    margin-right: 20px;
}

#barraTopo ul li a
{
    color:#6C3600;
    text-decoration:none;
}

#barraTopo ul li a:hover
{
    color:#CC6600;
}

#barraTopo ul li ul
{
    margin-top: 0.45em;
    background-color:#FFFFFF;
    margin-top:0px;
    padding:0px;
    position: absolute;
    z-index: 10;
    top: 1.5em;
    left: 0;
}

#barraTopo ul li ul li
{
    background-color:#FFCC66;
    width: 12em;
    display:block;
    padding: 0.15em;
    padding-left: 0.4em;
    margin-top: 1px;
    margin-right:0;
    z-index: 10;
}

#barraTopoCaminho
{
    margin: 20px 30px 0px;
    text-align:left;
	background: url(../img/house.png) no-repeat 0% 1px;
