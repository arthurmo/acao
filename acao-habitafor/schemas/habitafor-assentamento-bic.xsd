									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="poluicaoAr" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Poluição do ar</xhtml:label>
								</xs:appinfo>
