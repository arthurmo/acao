									<xhtml:label>Localização cartográfica</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="poligonalPontosGeoreferenciados"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Poligonal com pontos georeferenciados
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="tempoExistencia" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tempo de existência(em meses)</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="principaisViasAcesso" minOccurs="0">
