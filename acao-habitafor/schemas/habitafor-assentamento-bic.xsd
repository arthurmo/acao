								<xs:appinfo>
									<xhtml:label>Principais vias de acesso</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="situacaoFundiaria" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Situação fundiária</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
