						<xs:element name="existeConflitoFundiario" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Existencia de conflito fundiário</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="numeroUnidades" minOccurs="0">
