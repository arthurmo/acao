<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://schemas.fortaleza.ce.gov.br/habitafor/viladomar-cadernoa.xsd"
	targetNamespace="http://schemas.fortaleza.ce.gov.br/habitafor/viladomar-cadernoa.xsd"
	elementFormDefault="qualified">

	<xs:element name="formCadernoA" type="formCadernoA">
		<xs:annotation>
			<xs:appinfo>
				<label>Proposta de Cadastro Único</label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="formCadernoA">
		<xs:sequence>

			<xs:element name="origemDoEncaminhamento">
				<xs:annotation>
					<xs:appinfo>
						<label>Origem do Encaminhamento</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>

						<xs:element name="associacoes" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Associações</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="conselhoTutelar" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Conselho Tutelar</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="demandaEspontanea" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Demanda Espontânea</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="judiciario" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Judiciário</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="outrasInstituicoesOuProjetos" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Outras Instituições ou Projetos</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="programasOuProjetosDaFunci" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Programas ou Projetos da FUNCI</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="vinculacaoNaCca">
				<xs:annotation>
					<xs:appinfo>
						<label>Vinculacao Na CCA</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="adolescenteCidadao" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Adolescente Cidadao</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="aquarela" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Aquarela</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="bromelia" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Bromélia</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="casaDasMeninas" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Casa das Meninas</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="casaDosMeninos" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Casa dos Meninos</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="cozinhaExperimental" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Cozinha Experimental</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="crescerComArteCidadania" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Crescer com Arte Cidadania</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="disqueDireitosCriacaoEAdolescentes"
							type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Disque Direitos Criação e Adolescentes - DDCA</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="erradicacaoDoTrabalhoInfantil" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Erradicação do Trabalho Infantil</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="estiloSolidario" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Estilo Solidário</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="ponteDeEncontro" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Ponte de Encontro</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="liberdadeAssitida" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Liberdade Assitida - Se Garanta</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="preservacaoDeServicosAComunidade"
							type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<label>Preservação de Serviços à Comunidade - Se Garanta
									</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="dataEncaminhamento" type="xs:dateTime"
				minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<label>Data do Encaminhamento</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="status" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<label>Status</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Ativo" />
						<xs:enumeration value="Desligado/Ativado" />
						<xs:enumeration value="Esperando Engajamento" />
						<xs:enumeration value="Dessitência" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="identificacaoPessoal">
				<xs:annotation>
					<xs:appinfo>
						<label>Identificação Pessoal</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>

						<xs:element name="nomeCompleto" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Nome Completo</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="nomeSocial" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Nome Social</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="idade" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>idade</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="naturalidade" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>naturalidade</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="dataNascimento" type="xs:date"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Data de Nascimento</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="sexo" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Sexo</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Masculino" />
									<xs:enumeration value="Feminino" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="orientacaoSexual" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<label>Orientação Sexual</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Heterosexual" />
						<xs:enumeration value="Homosexual" />
						<xs:enumeration value="Bisexual" />
						<xs:enumeration value="Transexual" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="epecifiqueOutrosOrientacaoSexual" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<label>Especifique outros</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="filiacao">
				<xs:annotation>
					<xs:appinfo>
						<label>Filiação</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="pai" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Pai</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="mae" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Mãe</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="responsavel" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Responsável</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="endereco" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Endereço</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="pontoDeReferencia" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Ponto de Referencia</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="bairro" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Bairro</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="Regional" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Regional</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="telefones">
							<xs:annotation>
								<xs:appinfo>
									<label>Telefones</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="fixoResidencial" type="xs:string"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<label>Fixo(Residencial)</label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="publico" type="xs:string"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<label>Público</label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="celular" type="xs:string"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<label>Celular</label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="parentesAmigosConhecidos" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Parentes / Amigos / Conhecidos</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="filhos">
				<xs:annotation>
					<xs:appinfo>
						<label>Filhos(as)</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="temFilhos" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Tem Filhos</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="quantos" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Quantos</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="idade" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Idade</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="comQuemMora" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Com quem mora(m)</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="comQueIdadeTevePrimeiroFilho" type="xs:integer"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Com que idade teve o primeiro filho</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="racaEtinia" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<label>Raça/Etnia</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Branco" />
						<xs:enumeration value="Pardo" />
						<xs:enumeration value="Negro" />
						<xs:enumeration value="Indígena" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="estadoCivil" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<label>Estado Civil</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Solteiro" />
						<xs:enumeration value="Casado" />
						<xs:enumeration value="Separado" />
						<xs:enumeration value="Viúvo" />
						<xs:enumeration value="União Estável" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="possuiAlgumaDeficiencia" minOccurs="0">
				<xs:annotation>
					<xs:appinfo>
						<label>Possui alguma deficiência</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Sensorial Surdo" />
						<xs:enumeration value="Sensorial Cego" />
						<xs:enumeration value="Deficiência Físico/Motor" />
						<xs:enumeration value="Mobilidade Reduzida" />
						<xs:enumeration value="Deficiência Intelectual" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="documentacao">
				<xs:annotation>
					<xs:appinfo>
						<label>Documentação</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="registroDeNascimento" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Rgistro de nascimento</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="OK" />
									<xs:enumeration value="Não Tem" />
									<xs:enumeration value="Solicitado" />
									<xs:enumeration value="Não se Aplica" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="identidade" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Identidade/RG</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="OK" />
									<xs:enumeration value="Não Tem" />
									<xs:enumeration value="Solicitado" />
									<xs:enumeration value="Não se Aplica" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="cpf" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>CPF</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="OK" />
									<xs:enumeration value="Não Tem" />
									<xs:enumeration value="Solicitado" />
									<xs:enumeration value="Não se Aplica" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="ctps" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>CTPS</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="OK" />
									<xs:enumeration value="Não Tem" />
									<xs:enumeration value="Solicitado" />
									<xs:enumeration value="Não se Aplica" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="tituloDeEleitor" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Título de Eleitor</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="OK" />
									<xs:enumeration value="Não Tem" />
									<xs:enumeration value="Solicitado" />
									<xs:enumeration value="Não se Aplica" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="reservista" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Reservista</label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="OK" />
									<xs:enumeration value="Não Tem" />
									<xs:enumeration value="Solicitado" />
									<xs:enumeration value="Não se Aplica" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="convenienciaFamiliareComunitaria">

				<xs:annotation>
					<xs:appinfo>
						<label>Convêniencia Familiar e Comunitária</label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>
					<xs:sequence>

						<xs:element name="nome" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Nome</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="parentesco" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>*Parentesco</label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Mae" />
									<xs:enumeration value="Pai" />
									<xs:enumeration value="Irmã(o)" />
									<xs:enumeration value="Padrasto" />
									<xs:enumeration value="Madrasta" />
									<xs:enumeration value="Esposo(a)" />
									<xs:enumeration value="Namorado(a)" />
									<xs:enumeration value="Primo(a)" />
									<xs:enumeration value="Avô Materno" />
									<xs:enumeration value="Avó Materna" />
									<xs:enumeration value="Avô Paterno" />
									<xs:enumeration value="Avó Paterna" />
									<xs:enumeration value="Outros" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="idade" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>Idade</label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="escolaridade" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<label>**Escolaridade</label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Alfabetizado" />
									<xs:enumeration value="Não Alfabetizado" />
									<xs:enumeration value="Ensino Fundamental" />
									<xs:enumeration value="Ensino Médio" />
									<xs:enumeration value="Técnico" />
