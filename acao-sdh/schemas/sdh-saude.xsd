<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://schemas.fortaleza.ce.gov.br/habitafor/sdh-saude.xsd"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	targetNamespace="http://schemas.fortaleza.ce.gov.br/habitafor/sdh-saude.xsd"
	elementFormDefault="qualified">

	<xs:element name="formSaude" type="formSaude">
		<xs:annotation>
			<xs:appinfo>
				<xhtml:label>SAÚDE</xhtml:label>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="formSaude">
		<xs:sequence>

			<xs:element name="saudeDaFamilia">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Saúde da Familia</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>

						<xs:element name="nome" type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Nome</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="parentesco" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Parentesco</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="idade" type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Idade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="tipoDeDoenca" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tipo de doença</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="situacaoDetratamento" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Situacao de tratamento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Iniciado" />
									<xs:enumeration value="Suspenso" />
									<xs:enumeration value="Concluído" />
									<xs:enumeration value="Em andamento" />
									<xs:enumeration value="Outros" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="especifiqueOutrosSituacaoDeTratamento"
							type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Especifique outros situação de tratamento </xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="localDeTratamento" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Local de tratamento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="observacoes" type="xs:string"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Observações</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="suaFamiliaVaiAtendimentoMedicoRegularmente"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Sua família vai ao atendimento médico regularmente
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
									<xs:enumeration value="Não sei" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="especifiqueQuantasVezesAFamiliaVaiAtendimentoMedico"
							type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Especifique quantas vezes </xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="recebeMedicamentoQuandonecessario"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Recebe medicamento quando necessário</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
									<xs:enumeration value="Não sei" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element
							name="aFamiliaPossuiPessoasComHistoricoDeTranstornosMentais"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>A familia possui pessoas com histórico de transtornos
										mentais</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
									<xs:enumeration value="Não sei" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element
							name="especifiqueQuemdaFamiliaPossuiHistoricoDeTranstornosMentais"
							type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Especifique quem da familia possui histórico de
										transtornos mentais </xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="recebeAlgumAcompanhamentoInstitucional"
							type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Recebe algum acompanhamento institucional</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element
							name="especifiqueAQuantoTemporecebeAlgumAcompanhamentoInstitucional"
							type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Especifique a quanto tempo recebe algum
										acompanhamento institucional</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="onderecebeAlgumAcompanhamentoInstitucional"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Onde recebe algum acompanhamento
										institucional</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="CAPS" />
									<xs:enumeration value="Hospital mental" />
									<xs:enumeration value="Não recebe acompanhamento" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element
							name="especifiqueOnderecebeAlgumAcompanhamentoInstitucional"
							type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Especifique onde recebe algum acompanhamento
										institucional</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="naFamiliaExistemPessoasComAlgumaDeficiencia"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Na família existem pessoas com alguma deficiência
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="tipoDeDeficienciaComPessoasDaFamilia"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Tipo de deficiência com pessoas da família</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sensorial Surdo" />
									<xs:enumeration value="Sensorial Cego" />
									<xs:enumeration value="Deficiência Físico/Motor" />
									<xs:enumeration value="Mobilidade Reduzida" />
									<xs:enumeration value="Deficiência Intelectual" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="asCriancasEstaoComVacinasEmDia"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>As crianças estão com as vacinas em dias</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="especifiqueAsvacinasQueAsCriancasTomaram"
							type="xs:integer" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Especifique quais vacinas as crianças tomaram</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element
							name="algumMembroDaFamiliaOuSocioeducandoFazUsoDeSubstanciasPsicoativas"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Algum membro da família e/ou socioeducando faz uso de
										substâncias psicoativas</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:complexType>
								<xs:sequence>

									<xs:element
										name="parentescoOuSocioeducandoFazUsoDeSubstanciasPsicoativas"
										type="xs:integer" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Parentesco de quem faz uso de
													substâncias
													psicoativas</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouAlcool"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de alcool</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouCigarro"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de cigarro</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouMaconha"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de maconha</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouCocaina"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de cocaína</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouMesclado"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de mesclado</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouCrack"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de crack</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouComprimidos"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de comprimidos</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouCola"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de cola</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="parentescoOuSocioeducandoJaUsouInalantes"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quanto ao uso de inalantes</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Nunca usou" />
												<xs:enumeration value="Já experimentou" />
												<xs:enumeration value="Fez uso" />
												<xs:enumeration value="Consume de vez em quando" />
												<xs:enumeration value="Consume freqüentemente" />
												<xs:enumeration value="Não quis informar" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

								</xs:sequence>
							</xs:complexType>
						</xs:element>


						<xs:element name="acompanhamentoEspecificoParausoDrogras"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Acompanhamento específico para uso drogas</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:complexType>
								<xs:sequence>

									<xs:element name="nuncaFezAcompanhamento" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Nunca fez acompanhamento</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="FazAcompanhamento" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label> Onde faz acompanhamento</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>

												<xs:element name="CAPSad" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>CAPSad</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="hospitalMental" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Hospital mental</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>

									<xs:element name="especifiqueOutrosLocaisQueFazAcompanhamento"
										type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Especifique onde faz acompanhamento</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="especifiqueAQuantoTempoFazAcompanhamento"
										type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Especifique a quanto tempo faz acompanhamento
															</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="FezAcompanhamento" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label> Onde fez acompanhamento</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>

												<xs:element name="CAPSad" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>CAPSad</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="hospitalMental" type="xs:boolean">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Hospital mental</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

											</xs:sequence>
										</xs:complexType>
									</xs:element>

									<xs:element name="especifiqueOutrosLocaisQueFezAcompanhamento"
										type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Especifique onde fez acompanhamento</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="especifiqueAQuantoTempoFezAcompanhamento"
										type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Especifique a quanto tempo fez acompanhamento
															</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="jaInternadoEmAlgumaComunidadeTerapeutica"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Já internado em alguma comunidade terapêutica</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="especifiqueUnidadeterapeuticaQueficouInternado"
							type="xs:string" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Especifique unidade terapêutica que ficou internado
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="desejaTratamento" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Deseja tratamento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
