										</xs:annotation>
									</xs:element>

									<xs:element name="criadolviolifam1317new"  minOccurs="0" type="xs:integer">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>13 a 17 anos(novos)</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="crianadolabussex"  minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Crianças ou adolescentes vítimas de abuso sexual *</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
