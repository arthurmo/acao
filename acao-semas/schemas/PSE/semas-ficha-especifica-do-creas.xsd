						<xs:maxLength value="11"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="numerocpf" minOccurs="1">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>CPF*</xhtml:label>
