									</xs:element>

									<xs:element name="criadolviolifam012"  minOccurs="0" type="xs:integer">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>13 a 17 anos</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

