						<xs:element name="abandono" minOccurs="0" type="xs:boolean">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.4 Abandono</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="neglig" minOccurs="0" type="xs:boolean">
							<xs:annotation>
