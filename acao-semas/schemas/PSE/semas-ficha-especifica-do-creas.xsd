								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Identificada" />
									<xs:enumeration value="Não-identificada" />
									<xs:enumeration value="Auto-negligente" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="qdeagressores"  minOccurs="1" type="xs:integer">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>1.2 Quantidade de agressores *</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="residemesmodom"  minOccurs="1">
