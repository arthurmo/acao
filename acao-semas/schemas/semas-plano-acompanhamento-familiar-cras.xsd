			<xs:appinfo>
				<xhtml:label>PLANO ACOMPANHAMENTO FAMILIAR</xhtml:label>
				<class:classificacoes>
					<class:classificacao>cn=Proteção Social Básica,cn=Assistencia Social</class:classificacao>
				</class:classificacoes>
				<idx:index>
					<idx:hint key="pessoa.nome"	xpath="formPlanoAcompanhamentoFamiliar/nomeResponsavelFamiliar" />
					<idx:hint key="pessoa.nome"	xpath="formPlanoAcompanhamentoFamiliar/superacaoVulnerabilidade/responsavel" />
					<idx:hint key="pessoa.nome"	xpath="formPlanoAcompanhamentoFamiliar/tecResp" />
				</idx:index>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

