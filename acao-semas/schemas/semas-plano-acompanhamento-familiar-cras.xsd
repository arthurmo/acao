											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="ausenciaEnvolvidos" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Membros envolvidos</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="desemprego" type="xs:boolean">
