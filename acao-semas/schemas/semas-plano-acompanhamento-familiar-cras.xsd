										</xs:simpleType>
									</xs:element>
									<xs:element name="fragilPertencimentoSocial" type="xs:boolean">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Fragilização do pertencimento social
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
									<xs:element name="fragilPertencimentoSocialEnvolvidos"
										minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Membros envolvidos</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
