					<xs:appinfo>
						<xhtml:label>Trabalho de superação das vulnerabilidades
						</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="compTrabVulnerabilidade" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Vulnerabilidade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Pobreza" />
