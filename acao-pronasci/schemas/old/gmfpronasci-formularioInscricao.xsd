<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	xmlns="http://schemas.fortaleza.ce.gov.br/acao/gmfpronasci-formulario-inscricao.xsd"
	xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
	targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/gmfpronasci-formulario-inscricao.xsd"
	elementFormDefault="qualified">

	<xs:element name="form" type="formFormularioInscricao">
		<xs:annotation>
			<xs:appinfo>
				<xhtml:label>FORMULÁRIO INSCRIÇÃO  MULHERES DA PAZ</xhtml:label>
				<class:classificacoes>
					<class:classificacao>cn=Mulheres da Paz,cn=Direitos Humanos</class:classificacao>
				</class:classificacoes>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="formFormularioInscricao">
		<xs:sequence>
			<xs:element name="dadosPessoais">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1. Dados Pessoais</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="nome" minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Nome</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="dataNascimento" type="xs:date"
							minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data Nascimento</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="registroGeral" minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>RG</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="dataExpedicao" type="xs:date"
							minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Data expedição</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="orgaoExpedidor" minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Órgão</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="cpf" minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>CPF</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="titulo" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Título de Eleitor</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="endereco" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Endereço</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="bairro" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Bairro</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="cep" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>CEP</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="email" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>E-mail</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<!-- REFERÊNCIAS PESSOAIS -->
			<xs:element name="referenciasPessoais">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>Referências Pessoais</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="referenciaVisinho" minOccurs="1"
							maxOccurs="3">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Vizinhos</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="nome">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Nome</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="telefone">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Telefone</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="referenciaTrabalho" minOccurs="1"
							maxOccurs="3">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Do Trabalho</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="nome">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Nome</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="telefone">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Telefone</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<!--DADOS DE ESCOLARIDADE -->
			<xs:element name="escolaridade">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>2. Escolaridade</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>

					<xs:sequence>

						<xs:element name="escolaridade">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.1 Escolaridade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Ensino básico (1ª a 4ª série) completo" />
									<xs:enumeration value="Ensino básico (1ª a 4ª série) incompleto" />
									<xs:enumeration value="Ensino fundamental (5ª a 8ª série) completo" />
									<xs:enumeration value="Ensino fundamental (5ª a 8ª série) incompleto" />
									<xs:enumeration value="Ensino médio (2º grau) completo" />
									<xs:enumeration value="Ensino médio (2º grau) incompleto" />
									<xs:enumeration value="Ensino superior completo" />
									<xs:enumeration value="Ensino superior incompleto" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="estudaAtualmente">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.2 Estuda atualmente(escola, curso,
										profissionalizante etc.)?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="turno">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>2.3 Em que turno você estuda atualmente?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Manhã" />
									<xs:enumeration value="Tarde" />
									<xs:enumeration value="Noite" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>

			</xs:element>

			<!-- DADOS DA CONFIGURAÇÃO FAMILIAR -->
			<xs:element name="configuracaoFamiliar">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>3. Configuração Familiar</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="estadoCivil">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Estado Civil
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Solteira" />
									<xs:enumeration value="Casada" />
									<xs:enumeration value="União estável" />
									<xs:enumeration value="Viúva" />
									<xs:enumeration value="Divorciada" />
									<xs:enumeration value="Casada" />
									<xs:enumeration value="Separada judicialmente" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="filhos">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Filho(s)</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Não" />
									<xs:enumeration value="Sim" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="quantidadeFilhos" type="xs:integer">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quantos?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="idadeFilhos">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quais as idades deles?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="comQuemMora">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Com Quem Você Mora?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>

			</xs:element>

			<xs:element name="trabalho">

				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>4. Trabalho</xhtml:label>
					</xs:appinfo>
				</xs:annotation>

				<xs:complexType>

					<xs:sequence>

						<xs:element name="trabalhaAtualmente">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Está Trabalahando Atualmente?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="ondeTrabalha" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Onde Você Trabalha?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="queFaz" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>O Que Faz?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="horarioTrabalho">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Qual o seu Horário de Trabalho?(dia e horário)
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="valorSalario" type="xs:decimal">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Qual o valor do seu salário?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="quemTrabalha" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quem trabalha na sua família e quanto ganha?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>

						<xs:element name="cadastroUnicoGoverno">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Você/sua família estão no Cadastro Único do
										Governo Federal?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="recebemBeneficios">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Você/sua família recebem benefício social?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="profissional">

							<xs:annotation>
								<xs:appinfo>
									<xhtml:label></xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:complexType>

								<xs:sequence>

									<xs:element name="ultimaExperienciaProfissional"
										type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>4.1 Se você não está trabalhando atualmente,
													qual foi
													sua última experiência profissional?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>

									</xs:element>

									<xs:element name="rendaMediaFamilia">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>4.2 Você confirma que a renda média da sua
													família é menor ou igual a 2 salários mínimos
													(R$930,00 –
													Novecentos e
													trinta reais)?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>

										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>

									</xs:element>

								</xs:sequence>
							</xs:complexType>

						</xs:element>

						<!-- TIPO DE ATUAÇÃO NA COMUNIDADE -->

						<xs:element name="atuacaoComunidade">

							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>5. Tipo de atuação na comunidade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>

							<xs:complexType>

								<xs:sequence>

									<xs:element name="participa">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Participa de movimento/organização comunitária
													(Conselhos, Comissões, Comitês, Grupos de
													Trabalho, Fóruns,
													Associações, trabalho voluntário)?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>

										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
												<xs:enumeration value="Já Participei" />
											</xs:restriction>
										</xs:simpleType>

									</xs:element>

									<xs:element name="participaMovimentos" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="tempoParticipacao">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Há quanto tempo?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:maxLength value="255" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>

						</xs:element>

						<xs:element name="disponibilidadeProjeto">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>6. Disponibilidade para o projeto</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="descricaoDisponibilidadeProjeto" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="sobreAtuacaoComunidade">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>7. Fale sobre sua atuação na sua comunidade
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="descricaoSobreAtuacaoComunidade" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="razaoParaSerMulherDaPaz">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>8. Porque razão você deve ser uma Mulher da Paz?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="descricaoRazaoParaSerMulherDaPaz" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>

					</xs:sequence>
				</xs:complexType>

			</xs:element>

		</xs:sequence>

	</xs:complexType>

</xs:schema>
