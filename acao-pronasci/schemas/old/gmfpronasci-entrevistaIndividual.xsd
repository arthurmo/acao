<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xhtml="http://www.w3.org/1999/xhtml"
	xmlns="http://schemas.fortaleza.ce.gov.br/acao/gmfpronasci-entrevista-individual.xsd"
	xmlns:class="http://schemas.fortaleza.ce.gov.br/acao/classificacao.xsd"
	targetNamespace="http://schemas.fortaleza.ce.gov.br/acao/gmfpronasci-entrevista-individual.xsd"
	elementFormDefault="qualified">

	<xs:element name="form" type="formEntrevistaIndividual">
		<xs:annotation>
			<xs:appinfo>
				<xhtml:label>ENTREVISTA INDIVIDUAL MULHERES DA PAZ</xhtml:label>
				<class:classificacoes>
					<class:classificacao>cn=Mulheres da Paz,cn=Direitos Humanos</class:classificacao>
				</class:classificacoes>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>

	<xs:complexType name="formEntrevistaIndividual">

		<xs:sequence>

			<xs:element name="estabilidade">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1.1 ESTABILIDADE</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="nome" minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Nome</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>

						<xs:element name="apelido" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Apelido</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="idade" minOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Idade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="estadoCivil">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Estado Civil
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Solteira" />
									<xs:enumeration value="Casada" />
									<xs:enumeration value="União Estável" />
									<xs:enumeration value="Viúva" />
									<xs:enumeration value="Divorciada" />
									<xs:enumeration value="Casada" />
									<xs:enumeration value="Separada judicialmente" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="filhos">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Filho(s)
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Não" />
									<xs:enumeration value="Sim" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="quantidadeFilhos">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quantos?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="idadeFilhos">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quais as idades deles?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:maxLength value="255" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ajudaAsCriancas" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quem ajuda a cuidar das criancas?</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="moradia">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Sua casa é:
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Própria" />
									<xs:enumeration value="Alugada" />
									<xs:enumeration value="Cedida" />
									<xs:enumeration value="Coabitada" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ondeMorouAntes" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Onde você morou antes de vir para cá?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="tempoMoradiaAnterior" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Por quanto tempo morou lá?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="tempoMoradiaAtual" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Há quanto tempo mora no bairro?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="pretendePermanecerNobairro">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Pretende permanecer no bairro pelos próximos 12 ou
										2 meses?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Sim" />
									<xs:enumeration value="Não" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="parenteNoBairro" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quem da família mora do bairro?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="quemTrabalha" type="xs:string">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Quem trabalha na sua família e quanto ganha?
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="capcidadeLideranca">
				<xs:annotation>
					<xs:appinfo>
						<xhtml:label>1.2 CAPACIDADE DE LIDERANÇA</xhtml:label>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="conhecimentoBairro">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Se conhece o bairro</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="conhecePostosSaude" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece postos de saúde?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="postosSaudeConhece" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conheceConselhosTutelares"
										minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece conselhos tutelares?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="conselhosTutelaresConhece" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conhecePracasQuadras" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece praças ou quadras de esportes?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="pracasQuadrasConhece" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conheceDelegacias" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece delegacias no seu bairro?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="delegaciasConhece" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="conheceIgrejaTemplos" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você conhece igrejas ou templos no seu bairro?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="igrejaTemplosConhece" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quais?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

								</xs:sequence>
							</xs:complexType>

						</xs:element>

						<xs:element name="promocaoAtividade">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Se promove/ajuda a promover alguma atividade
									</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="promoveuFestas" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você já promoveu alguma festa em seu bairro?
												</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="festasPromoveu" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Qual?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="auxiliouEventosEscola" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você já auxiliou algum evento na escola em que
													estudava?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="eventosEscola" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Qual?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

									<xs:element name="auxiliouEventosIgreja" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você já auxiliou algum evento na igreja ou
													templo?
									</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Sim" />
												<xs:enumeration value="Não" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="eventosIgreja" type="xs:string">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Qual?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
									</xs:element>

								</xs:sequence>

							</xs:complexType>

						</xs:element>

						<xs:element name="participacaoEntidadeOuProjeto">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Se participa de entidade ou projeto</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>

									<xs:element name="participaEntidadeProjeto">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Você participa de:</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="participaAssociacaoComunitaria"
													minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Associação comunitária
												</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="associacaoComunitaria" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaPastorais" minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Pastorais
												</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="pastorais" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaConselhos" minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Conselhos(comunitários, de defesa social,
																escolares, outros)
												</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="conselhos" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaProjetos" minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Projetos</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="projetos" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaOngs" minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>ONGs</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="ongs" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaCentrosDefesa"
													minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Centros de defesa</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>

												<xs:element name="centrosDefesa" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaMovimentosSaudeMental"
													minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Movimentos de saúde mental</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="movimentosSaudeMental" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>

												<xs:element name="participaOutros" minOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Outros</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Sim" />
															<xs:enumeration value="Não" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="outros" type="xs:string">
													<xs:annotation>
														<xs:appinfo>
															<xhtml:label>Qual?</xhtml:label>
														</xs:appinfo>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>

									</xs:element>

								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="referenciaComunidade">
							<xs:annotation>
								<xs:appinfo>
									<xhtml:label>Se é referenciada pela comunidade</xhtml:label>
								</xs:appinfo>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="quandoAlguemFicaDoente" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quando alguém fica doente na sua rua, você sabe
													como ela faz?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Procura posto de saúde" />
												<xs:enumeration value="Liga para a ambulância" />
												<xs:enumeration value="Procura você" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>

									<xs:element name="quandoPrecisaAdvogado" minOccurs="1">
										<xs:annotation>
											<xs:appinfo>
												<xhtml:label>Quando alguém precisa de advogado, você sabe
													como
													ela faz?</xhtml:label>
											</xs:appinfo>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
